{
  "compilerOptions": {
    "baseUrl": "./",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "target": "ES2018",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "outDir": "./dist/exec",
    "strictNullChecks": false,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "declaration": true,
    "paths": {
      "rws-js-server": [
        "../src"
      ]
    }
  },
  "include": [
    "src"
  ],
  "exclude": []
}